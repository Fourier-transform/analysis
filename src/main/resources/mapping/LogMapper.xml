<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.legend.analysis.mapper.LogMapper">
  <resultMap id="BaseResultMap" type="com.legend.analysis.mapper.Log">
    <result column="id" jdbcType="VARCHAR" property="id" />
    <result column="field1" jdbcType="VARCHAR" property="field1" />
  </resultMap>
  <insert id="insert" parameterType="com.legend.analysis.mapper.Log">
    insert into t_log (id, field1)
    values (#{id,jdbcType=VARCHAR}, #{field1,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="com.legend.analysis.mapper.Log">
    insert into t_log
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="id != null">
        id,
      </if>
      <if test="field1 != null">
        field1,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="id != null">
        #{id,jdbcType=VARCHAR},
      </if>
      <if test="field1 != null">
        #{field1,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
</mapper>